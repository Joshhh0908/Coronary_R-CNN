conda create -n RCNN python=3.8 -y
conda activate RCNN
conda install pytorch==1.8.1 torchvision==0.9.1 torchaudio==0.8.1 cudatoolkit=11.1 -c pytorch-lts -c nvidia

pip install opencv-python
